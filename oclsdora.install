<?php

/**
 * @file
 * Install hooks for oclsdora.
 */

/**
 * Implements hook_uninstall().
 */
function oclsdora_uninstall() {
  $variables = array(
    'islandora_guided_ingest_next_prev',
    'islandora_guided_ingest_next_text',
    'islandora_guided_ingest_prev_text',
  );
  array_walk($variables, 'variable_del');
}
